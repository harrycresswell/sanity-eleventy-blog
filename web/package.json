{
  "name": "sanity-eleventy-blog-web",
  "version": "1.0.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "now-build": "npm run build",
    "dev": "run-p watch:eleventy watch:parcel bs:start",
    "watch:eleventy": "npx eleventy --watch",
    "watch:parcel": "run-p watch:css watch:js",
    "watch:css": "parcel watch src/scss/styles.scss",
    "watch:js": "parcel watch src/js/index.js",
    "build": "run-s clean prod:parcel prod:eleventy",
    "prod:eleventy": "npx eleventy",
    "prod:parcel": "run-p build:css build:js",
    "build:css": "parcel build src/scss/styles.scss",
    "build:js": "parcel build src/js/index.js",
    "bs:start": "browser-sync start --config bs-config.js",
    "debug": "DEBUG=* npx eleventy",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@sanity/block-content-to-html": "^1.3.8",
    "@sanity/block-content-to-markdown": "0.0.5",
    "@sanity/client": "^2.0.0",
    "browser-sync": "^2.26.14",
    "clean-css": "^4.2.1",
    "dotenv": "^8.2.0",
    "groq": "^0.142.0",
    "luxon": "^1.19.3",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.32.8"
  },
  "dependencies": {}
}
